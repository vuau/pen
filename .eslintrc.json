{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "standard",
  "plugins": ["svelte3"],
  "globals": {
    "alert": "readonly",
    "localStorage": "readonly"
  },
  "overrides": [
    {
      "files": ["**/*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "rules": {
    "import/first": "off",
    "import/no-duplicates": "off",
    "import/no-mutable-exports": "off",
    "import/no-unresolved": "off",
    "no-multiple-empty-lines": "off"
  }
}

